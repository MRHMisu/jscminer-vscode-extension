{
    "name": "jscminer",
    "displayName": "JSCMiner",
    "description": "Clone Detector for JavaScript Applicaiton",
    "version": "0.0.1",
    "publisher": "MisuBeImp",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.detectClones"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.detectClones",
            "title": "Detect Clones"
        }]
    },"scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}